	J -> R
	P -> S
	Q -> T
	T -> U
	R -> V
	R -> W
	R -> X
	V -> Y
	W -> Z
	Z -> a
	X -> b
	X -> c
	X -> d
	b -> e
	c -> f
	f -> g
	d -> h
	d -> i
	d -> j
	h -> k
	i -> l
	l -> m
	j -> n
	j -> o
	n -> p
	o -> q
	q -> r
}
